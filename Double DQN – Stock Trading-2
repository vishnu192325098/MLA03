import numpy as np
import random

Q=np.zeros((10,3))
Q2=np.zeros((10,3))
alpha,gamma=0.1,0.9

for _ in range(500):
    s=random.randint(0,9)
    a=random.randint(0,2)
    r=random.randint(-5,5)
    s2=random.randint(0,9)
    a2=np.argmax(Q[s2])
    Q[s,a]+=alpha*(r+gamma*Q2[s2,a2]-Q[s,a])
    Q,Q2=Q2,Q

print(Q)
