import numpy as np
import random

states=10
actions=4
Q=np.zeros((states,actions))
alpha,gamma,epsilon=0.1,0.9,0.1

for _ in range(500):
    s=random.randint(0,states-1)
    a=random.randint(0,actions-1)
    for _ in range(20):
        s2=random.randint(0,states-1)
        r=1
        a2=random.randint(0,actions-1)
        Q[s,a]+=alpha*(r+gamma*Q[s2,a2]-Q[s,a])
        s,a=s2,a2

print(Q)
